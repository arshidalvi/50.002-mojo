module boole8 (
    input a[8],
    input b[8],
    input alufn[6],
    output out[8]
  ) {

  always {
   case(alufn[3:0]) {
    4b1000:
      out = a & b;
    4b1110:
      out = a | b;
    4b0110:
      out = a ^ b;
    4b1010:
      out = a;
    4b0101:
      out = b;
    default:
      out = 0;
    }
  }
}
